<h1>Listing car_plates</h1>

<table>
  <tr>
    <th>Country</th>
    <th>Validated</th>
    <th>Car ident num</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @car_plates.each do |car_plate| %>
  <tr>
    <td><%= car_plate.country %></td>
    <td><%= car_plate.validated %></td>
    <td><%= car_plate.car_ident_num %></td>
    <td><%= link_to 'Show', car_plate %></td>
    <td><%= link_to 'Edit', edit_car_plate_path(car_plate) %></td>
    <td><%= link_to 'Destroy', car_plate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car plate', new_car_plate_path %>
